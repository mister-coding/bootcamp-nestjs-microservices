    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/repositories(|/.*)$": "<rootDir>/libs/repositories/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@app/broker(|/.*)$": "<rootDir>/libs/broker/src/$1",
      "^@app/worker(|/.*)$": "<rootDir>/libs/worker/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/mail(|/.*)$": "<rootDir>/libs/mail/src/$1",
      "^@app/awss3(|/.*)$": "<rootDir>/libs/awss3/src/$1"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
