    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "5.16.1",
    "@sentry/nestjs": "^8.19.0",
    "aws-sdk": "^2.1677.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.12",
    "handlebars": "^4.7.8",
    "nats": "^2.28.0",
    "nodemailer": "^6.9.14",
